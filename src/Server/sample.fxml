<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.101" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Server.Controller">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="New" />
                        <MenuItem mnemonicParsing="false" text="Open…" />
                        <Menu mnemonicParsing="false" text="Open Recent" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Close" />
                        <MenuItem mnemonicParsing="false" text="Save" />
                        <MenuItem mnemonicParsing="false" text="Save As…" />
                        <MenuItem mnemonicParsing="false" text="Revert" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Preferences…" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Quit" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Undo" />
                        <MenuItem mnemonicParsing="false" text="Redo" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Cut" />
                        <MenuItem mnemonicParsing="false" text="Copy" />
                        <MenuItem mnemonicParsing="false" text="Paste" />
                        <MenuItem mnemonicParsing="false" text="Delete" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Select All" />
                        <MenuItem mnemonicParsing="false" text="Unselect All" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Label layoutX="14.0" layoutY="24.0" text="Server:" />
                <Label layoutX="14.0" layoutY="51.0" text="Port:" />
                <Label layoutX="13.0" layoutY="80.0" text="Status:" />
                <Label fx:id="labelServerStatus" layoutX="61.0" layoutY="80.0" text="Offline" />
                <Button fx:id="startButton" layoutX="13.0" layoutY="108.0" mnemonicParsing="false" onAction="#startAction" text="Start" />
                <Button fx:id="stopButton" layoutX="79.0" layoutY="108.0" mnemonicParsing="false" onAction="#stopServer" text="Stop" />
                <TextArea fx:id="areaLog" editable="false" focusTraversable="false" layoutX="13.0" layoutY="161.0" prefHeight="200.0" prefWidth="200.0" />
                <Label layoutX="13.0" layoutY="144.0" text="Server log:" />
                <Separator layoutX="236.0" layoutY="8.0" orientation="VERTICAL" prefHeight="362.0" prefWidth="5.0" />
                <Button fx:id="addButton" layoutX="258.0" layoutY="35.0" mnemonicParsing="false" onAction="#addClient" text="Add" />
                <Button fx:id="delButton" layoutX="315.0" layoutY="35.0" mnemonicParsing="false" onAction="#delClient" text="Delete" />
                <Button fx:id="correctButton" layoutX="383.0" layoutY="35.0" mnemonicParsing="false" onAction="#correction" text="Correct" />
                <Label layoutX="394.0" layoutY="16.0" text="Clients database" />
                <Separator layoutX="237.0" layoutY="170.0" prefHeight="4.0" prefWidth="402.0" />
                <Label layoutX="410.0" layoutY="181.0" text="Client info" />
                <Label layoutX="249.0" layoutY="206.0" text="Client name:" />
                <TextField fx:id="infaChel" layoutX="320.0" layoutY="202.0" />
                <Button layoutX="483.0" layoutY="202.0" mnemonicParsing="false" onAction="#showClient" text="Show" />
                <GridPane layoutX="295.0" layoutY="252.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label text="Name:" />
                        <Label text="Balance:" GridPane.rowIndex="1" />
                        <Label text="Status:" GridPane.rowIndex="2" />
                  <Label fx:id="infaName" text="Label" GridPane.columnIndex="1" />
                  <Label fx:id="infaBalance" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                    </children>
                </GridPane>
                <GridPane layoutX="258.0" layoutY="63.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label text="Name:" />
                        <Label text="Password:" GridPane.rowIndex="1" />
                        <Label text="Balance:" GridPane.rowIndex="2" />
                        <TextField fx:id="clientName" GridPane.columnIndex="1" />
                        <TextField fx:id="clientPassword" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <TextField fx:id="clientBalance" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                    </children>
                </GridPane>
            <Label fx:id="labelServerIP" layoutX="73.0" layoutY="24.0" prefHeight="17.0" prefWidth="123.0" />
            <Label fx:id="labelServerPort" layoutX="73.0" layoutY="52.0" prefHeight="17.0" prefWidth="100.0" />
            </children>
        </AnchorPane>
    </children>
</VBox>
